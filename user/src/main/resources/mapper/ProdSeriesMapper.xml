<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jxm.user.mapper.ProdSeriesMapper">

    <resultMap id="BaseResultMap" type="com.jxm.user.dao.SeriesModel">
        <id column="id" jdbcType="INTEGER" property="id"/>
        <result column="series_name" jdbcType="VARCHAR" property="seriesName"/>
        <result column="english_name" jdbcType="VARCHAR" property="englishName"/>
    </resultMap>

    <insert id="create" parameterType="com.jxm.user.dao.SeriesModel">
        insert into prod_series ( id, series_name,english_name)
        values (#{id,jdbcType=INTEGER},#{seriesName,jdbcType=VARCHAR},#{englishName,jdbcType=VARCHAR})
    </insert>

    <select id="listAll" resultType="com.jxm.user.dao.SeriesModel">
        select
        id, series_name,english_name
        from prod_series
    </select>

</mapper>