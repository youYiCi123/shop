<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jxm.business.mapper.TempUserMapper">

    <resultMap id="BaseResultMap" type="com.jxm.business.model.TempUserParam">
        <id column="id" jdbcType="BIGINT" property="id"/>
        <result column="user_id" jdbcType="BIGINT" property="userId"/>
        <result column="user_name" jdbcType="VARCHAR" property="userName"/>
        <result column="temp_id" jdbcType="BIGINT" property="tempId"/>
        <result column="temp_name" jdbcType="VARCHAR" property="tempName"/>
        <result column="temp_type" jdbcType="INTEGER" property="tempType"/>
        <result column="create_time" jdbcType="TIMESTAMP" property="createTime"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,user_id,user_name,temp_id,temp_name,temp_type,create_time
    </sql>

    <select id="selectByTempAndUser" resultType="com.jxm.business.model.TempUserParam">
        select
        <include refid="Base_Column_List"/>
        from temp_user
        where temp_id=#{tempId,jdbcType=BIGINT} AND user_id=#{userId,jdbcType=BIGINT}
    </select>

    <insert id="add" parameterType="com.jxm.business.model.TempUserParam">
        insert into temp_user
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null">
                id,
            </if>
            <if test="userId != null">
                user_id,
            </if>
            <if test="userName != null">
                user_name,
            </if>
            <if test="tempId != null">
                temp_id,
            </if>
            <if test="tempName != null">
                temp_name,
            </if>
            <if test="tempType != null">
                temp_type,
            </if>
            <if test="createTime != null">
                create_time
            </if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="id != null">
                #{id,jdbcType=BIGINT},
            </if>
            <if test="userId != null">
                #{userId,jdbcType=BIGINT},
            </if>
            <if test="userName != null">
                #{userName,jdbcType=VARCHAR},
            </if>
            <if test="tempId != null">
                #{tempId,jdbcType=BIGINT},
            </if>
            <if test="tempName != null">
                #{tempName,jdbcType=VARCHAR},
            </if>
            <if test="tempType != null">
                #{tempType,jdbcType=INTEGER},
            </if>
            <if test="createTime != null">
                #{createTime,jdbcType=TIMESTAMP}
            </if>
        </trim>
    </insert>

</mapper>